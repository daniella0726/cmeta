        event.preventDefault();
        document.getElementById("but_res_nombre").click();
      }
    });

  document.getElementById("pregunta_0_nombre_boton").addEventListener("click", fun_nombre_dev);
  function fun_nombre_dev() {
      $('#div_pregunta_0').slideDown();
      $('#div_pregunta_0').removeClass('hidden');
      $('#div_nombre').slideUp();
      $('#div_nombre').addClass('hidden');
      $('#pregunta_nombre_footer').addClass('hidden');
      $('#div_error_nombre').addClass('hidden');
  }

  document.getElementById("pregunta_semestre_nombre_boton").addEventListener("click", fun_nombre);
  document.getElementById("but_res_nombre").addEventListener("click", fun_nombre);
  function fun_nombre() {
    var frase = document.getElementById("res_nombre").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      frase = frase.replace("  "," ");
      document.getElementById("res_nombre").value = frase;
    if (document.getElementById("res_nombre").value == "") {
      $('#but_res_nombre').addClass('hidden');
      $('#p_nombre').addClass('hidden');
      $('#div_error_nombre').removeClass('hidden');
    } else {
      $('#div_nombre').slideUp();
      $('#div_nombre').addClass('hidden');
      $('#pregunta_nombre_footer').addClass('hidden');
      $('#div_semestre').slideDown();
      $('#div_semestre').removeClass('hidden');
      $('#pregunta_semestre_footer').removeClass('hidden');
      $('#div_error_semestre').addClass('hidden');
      document.getElementById("res_semestre").focus();
    }
  }

  $(document).ready(function(){ 

    //document.getElementById("but_res_pregunta_0").focus();

    $('#res_nombre').on('input', function() { 
      var frase = document.getElementById("res_nombre").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      frase = frase.replace("  "," ");
      document.getElementById("res_nombre").value = frase;
      if (document.getElementById("res_nombre").value == "") {
      $('#but_res_nombre').addClass('hidden');
      $('#p_nombre').addClass('hidden');
      $('#div_error_nombre').removeClass('hidden');
      } else {
       $('#but_res_nombre').removeClass('hidden');
       $('#p_nombre').removeClass('hidden');
       $('#div_error_nombre').addClass('hidden');
      }
    }); 
  }); 
</script>

